<h1> New Guest </h1>

<%= form_for(@guest) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

</br>
</br>

  <%= f.label :password %>
  <%= f.text_field :password %>

</br>
</br>

  <%= f.label :password_confirmation %>
